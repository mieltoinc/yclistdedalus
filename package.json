{
  "name": "yc-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for accessing YC company data",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
    "start": "node dist/index.js --port 3003",
    "dev:stdio": "tsx src/index.ts",
    "dev:shttp": "tsx src/index.ts --port 3003",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector dist/index.js"
  },
  "keywords": ["mcp", "yc", "startups", "companies"],
  "author": "citguru",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.16.0",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
